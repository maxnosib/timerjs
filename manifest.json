{
    "manifest_version": 2,

    "name": "timerjs",
    "description": "This extension get time after start",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}